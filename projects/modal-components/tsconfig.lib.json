/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "target": "ES2022",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "ES2022"
    ]
  },
  "angularCompilerOptions": {
    "preserveSymlinks": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ],
  "paths": {
    "@root/*": [
      "./*"
    ],
    "@themes/*": [
      "src/map/themes/*"
    ],
    "@map-plate/*": [
      "src/map/map-plate/*"
    ],
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ]
    }
  },
  "lib": [
    "ES2022",
    "dom"
  ]
}
